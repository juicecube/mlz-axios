{
  "name": "@mlz/axios",
  "version": "1.1.3",
  "description": "mlz-axios",
  "main": "dist/ts-axios.umd.js",
  "module": "dist/ts-axios.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "node examples/server.js",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest",
    "coverage": "jest --coverage",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "prepub": "npm run test:prod && npm run build",
    "release": "standard-version",
    "pub": "sh release.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juicecube/mlz-axios.git"
  },
  "author": "DogJun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juicecube/mlz-axios/issues"
  },
  "homepage": "https://github.com/juicecube/mlz-axios#readme",
  "dependencies": {
    "axios": "^0.21.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "/test.*\\.(test|spec)\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "jsx",
      "js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}",
      "src/**/*.{js,ts}"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "setupFiles": [],
    "setupFilesAfterEnv": [
      "<rootDir>/test/boot.ts"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@mlz/lint": "^1.1.10",
    "@types/jasmine-ajax": "^3.1.40",
    "@types/jest": "^24.0.18",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "husky": "^4.2.3",
    "jasmine-ajax": "^4.0.0",
    "jasmine-core": "^3.4.0",
    "jest": "^24.9.0",
    "jest-config": "^24.9.0",
    "lodash.camelcase": "^4.3.0",
    "rollup": "^1.20.1",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.23.0",
    "standard-version": "^9.3.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.39.3",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
