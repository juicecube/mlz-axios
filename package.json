{
  "name": "mlz-axios",
  "version": "1.0.0",
  "description": "",
  "main": "dist/ts-axios.umd.js",
  "module": "dist/ts-axios.es5.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juicecube/mlz-axios.git"
  },
  "author": "DogJun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juicecube/mlz-axios/issues"
  },
  "homepage": "https://github.com/juicecube/mlz-axios#readme",
  "dependencies": {
    "axios": "^0.19.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "/test.*\\.(test|spec)\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "jsx",
      "js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}",
      "src/**/*.{js,ts}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/boot.ts"
    ]
  },
  "devDependencies": {
    "@types/jasmine-ajax": "^3.1.40",
    "@types/jest": "^24.0.18",
    "express": "^4.17.1",
    "jasmine-ajax": "^4.0.0",
    "jasmine-core": "^3.4.0",
    "jest": "^24.9.0",
    "jest-config": "^24.9.0",
    "lodash.camelcase": "^4.3.0",
    "rollup": "^1.20.1",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.23.0",
    "ts-jest": "^24.0.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.3"
  }
}
